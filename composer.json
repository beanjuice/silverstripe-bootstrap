{
    "name": "codebunch/silverstripe-bootstrap",
    "description": "SilverStripe Bootstrap Recipe",
    "type": "project",
    "keywords": [
        "silverstripe",
        "bootstrap",
        "themes",
        "silverstripe setup",
        "silverstripe 4"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sathish",
            "email": "veluchamysathishkumar@gmail.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "^4.2",
        "dnadesign/silverstripe-elemental": "3.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "expose": [
            "themes/creative/css/",
            "themes/creative/webfonts/",
            "themes/creative/images/",
            "themes/creative/javascript/"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config",
            "assets/",
            "themes/creative/css/",
            "themes/creative/webfonts/",
            "themes/creative/images/",
            "themes/creative/javascript/"
        ]
    },
    "config": {
        "process-timeout": 600
    }
}
