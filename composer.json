{
    "name": "sathish-vel/silverstripe-bootstrap",
    "type": "silverstripe-bootstrap-boilerplate",
    "description": "SilverStripe Bootstrap Boilerplate",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.2.1@stable",
        "dnadesign/silverstripe-elemental": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "3.*"
    },
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Sathish Vel",
        "email": "veluchamysathishkumar@gmail.com"
    }],
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
